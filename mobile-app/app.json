{
  "expo": {
    "name": "Rhapsody TV",
    "slug": "rhapsody-tv",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icons/app-icon.png",
    "scheme": "rhapsodytv",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rhapsodytv.app"
    },
    "android": {
      "package": "com.rhapsodytv.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/Icons/app-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "useNextNotificationsApi": true,
      "permissions": [
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/Icons/app-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/Logo.png",
          "imageWidth": 261,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/logo/Logo.png",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/Icons/app-icon.png",
          "color": "#2563EB",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {}
    }
  }
}
